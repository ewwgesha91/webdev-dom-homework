{
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "extends": ["eslint:recomend", "prettier"],
    "plugins": ["prettier"],
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "rules": {
      "prettier": "error",
      "camelcase": ["error"],
      "eqeqeq": ["error", "always"]
    }
  }